<Window x:Class="StudentStatisticGroupe.Windows.GroupeAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentStatisticGroupe.Windows"
        mc:Ignorable="d"
        Title="GroupeAdd" Height="250" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBox Name="Name" Grid.Row="0" />
        <Button Name="btnSet" Grid.Row="1" Content="Загрузить" Margin="20" Click="BtnSet_Click" />
    </Grid>
</Window>
