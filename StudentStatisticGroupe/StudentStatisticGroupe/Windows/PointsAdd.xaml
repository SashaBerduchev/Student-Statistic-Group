<Window x:Class="StudentStatisticGroupe.Windows.PointsAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentStatisticGroupe.Windows"
        mc:Ignorable="d"
        Title="PointsAdd" Height="200" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox Name="pointtext" Grid.Row="0" Margin="20"/>
        <Button Name="btnSet" Grid.Row="1" Margin="20" Content="Сохранить" Click="BtnSet_Click"/>
    </Grid>
</Window>
