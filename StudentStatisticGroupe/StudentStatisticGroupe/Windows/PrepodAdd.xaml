<Window x:Class="StudentStatisticGroupe.Windows.PrepodAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentStatisticGroupe.Windows"
        mc:Ignorable="d"
        Title="PrepodAdd" Height="400" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox Name="name" Grid.Row="0" Margin="20"/>
        <TextBox Name="sename" Grid.Row="1" Margin="20"/>
        <TextBox Name="phone" Grid.Row="2" Margin="20"/>
        <ComboBox Name="discipline" Grid.Row="3" Margin="20"/>
        <TextBox Name="adress" Grid.Row="4" Margin="20"/>
        <Button Name="setPrepod" Grid.Row="5" Margin="15" Content="Загрузить" Click="SetPrepod_Click"/>
    </Grid>
</Window>
