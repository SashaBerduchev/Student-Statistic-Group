<Window x:Class="StudentStatisticGroupe.Windows.Prepods"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentStatisticGroupe.Windows"
        mc:Ignorable="d"
        Title="Prepods" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Name="addPrepod" Grid.Column="0" Grid.Row="0" Content="Карточка преподавателя" Margin="40" Click="AddPrepod_Click"/>
        <Button Name="loadPrepod" Grid.Column="1" Grid.Row="0" Content="Загрузить данные" Margin="40"/>
    </Grid>
</Window>
