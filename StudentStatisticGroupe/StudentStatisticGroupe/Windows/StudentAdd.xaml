<Window x:Class="StudentStatisticGroupe.Windows.StudentAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StudentStatisticGroupe.Windows"
        mc:Ignorable="d"
        Title="StudentAdd" Height="450" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBox Name="Name" Grid.Row="0" Margin="20"/>
        <TextBox Name="Serame" Grid.Row="1" Margin="20"/>
        <TextBox Name="Phone" Grid.Row="2" Margin="20"/>
        <ComboBox Name="Groupe" Grid.Row="3" Margin="20"/>
        <TextBox Name="Address" Grid.Row="4" Margin="20"/>
        <Button Name="ctnSet" Grid.Row="5" Margin="20" Click="CtnSet_Click" />
    </Grid>
</Window>
